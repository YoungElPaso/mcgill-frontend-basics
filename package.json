{
  "name": "mcGill-frontend-basics",
  "version": "1.0.0",
  "description": "A package that manages McGill frontend dependencies and some configuration and build steps.",
  "scripts": {
    "watch": "node-sass --include-path node_modules/susy/sass -r --output-style expanded  -w sass/ -o css/",
    "build": "node-sass --include-path node_modules/susy/sass -r --output-style compressed sass/ -o css/"
  },
  "keywords": [
    "css",
    "sass"
  ],
  "author": "Jesse A Sutherland",
  "license": "ISC",
  "dependencies": {
    "susy": "^3.0.5"
  },
  "devDependencies": {
    "node-sass": "git+https://github.com/sass/node-sass.git#v4.7.0"
  }
}
